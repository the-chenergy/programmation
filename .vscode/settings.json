{
  "[cpp]": {
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "cSpell.words": [
    "endregion",
    "infj",
    "nanj",
    "nextafter",
    "programmation",
    "randint",
    "setrecursionlimit",
    "sortedcontainers",
    "trav",
    "Wshadow",
    "YAPF"
  ],
  "code-runner.executorMap": {
    "cpp": "g++-12 -std=gnu++17 -Wno-fatal-errors -Wall -Wextra -pedantic -O2 -Wshadow -Wformat=2 -Wfloat-equal -Wconversion -Wlogical-op -Wshift-overflow=2 -Wduplicated-cond -Wcast-qual -Wcast-align -Wno-unknown-pragmas -DLOCAL -ftrapv -fno-sanitize-recover -fstack-protector $fullFileName && ./a.out",
    "python": "conda activate && python -u $fullFileName"
  },
  "editor.rulers": [80],
  "python.formatting.yapfArgs": ["--style={BASED_ON_STYLE: google}"],
  "workbench.colorCustomizations": {
    "activityBar.background": "#edd40040"
  }
}
